{
  "name": "maxtime",
  "version": "0.1.0",
  "author": {},
  "main": "./dist/main/main.js",
  "dependencies": {
    "fs-extra": "^7.0.0",
    "log4js": "^3.0.2",
    "process-utils": "file:addon/process-utils",
    "react": "^16.5.1",
    "react-dom": "^16.5.1"
  },
  "scripts": {
    "build": "npm run build:main && npm run build:renderer",
    "build:main": "tsc",
    "build:renderer": "webpack --config webpack.config.renderer.js",
    "start": "electron . --enable-logging",
    "package:dir": "npm run build && electron-builder --dir",
    "build:native": "install-app-deps"
  },
  "devDependencies": {
    "@types/fs-extra": "^5.0.4",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.7",
    "awesome-typescript-loader": "^5.2.0",
    "cross-env": "^5.2.0",
    "devtron": "^1.4.0",
    "electron": "2.0.7",
    "electron-builder": "^20.28.2",
    "electron-rebuild": "^1.8.1",
    "node-gyp": "^3.7.0",
    "source-map-loader": "^0.2.4",
    "tslint": "^5.10.0",
    "typescript": "^3.0.3",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0"
  },
  "build": {
    "productName": "maxtime",
    "compression": "maximum",
    "directories": {
      "buildResources": "./assets",
      "output": "./release"
    },
    "win": {
      "icon": "app.ico"
    },
    "electronDist": "./node_modules/electron/dist",
    "asar": true,
    "asarUnpack": [
      "./node_modules/process-utils"
    ],
    "extraResources": [
      "assets"
    ],
    "files": [{
        "from": "dist",
        "to": "dist",
        "filter": [
          "**/*",
          "!**/*.map"
        ]
      },
      {
        "from": ".",
        "to": ".",
        "filter": [
          "package.json"
        ]
      }
    ]
  }
}